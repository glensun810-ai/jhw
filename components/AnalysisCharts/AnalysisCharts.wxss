/* 分析图表主组件样式 - 简化版 */

.analysis-charts {
  display: flex;
  flex-direction: column;
  gap: 32rpx;
}

.chart-section {
  background: #fff;
  border-radius: 16rpx;
  padding: 32rpx;
}

.section-title {
  font-size: 32rpx;
  font-weight: 600;
  color: #333;
  margin-bottom: 24rpx;
  text-align: center;
}
